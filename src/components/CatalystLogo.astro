---
interface Props {
  size?: number;
  class?: string;
}

const { size = 24, class: className = '' } = Astro.props;
---

<svg
  width={size}
  height={size}
  viewBox="0 0 120 120"
  fill="none"
  xmlns="http://www.w3.org/2000/svg"
  class={className}
>
  <rect
    x="60"
    y="8"
    width="73.54"
    height="73.54"
    rx="4"
    transform="rotate(45 60 8)"
    stroke="currentColor"
    stroke-width="7"
  />
</svg>
